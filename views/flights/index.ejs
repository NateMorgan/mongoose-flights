<%- include('../partials/html-head.ejs') %>
<%- include('../partials/nav.ejs') %>
<h1><%= title %></h1>
<table>
  <thead>
    <tr>
      <th> Airline </th>
      <th> Airport </th>
      <th> Flight Number </th>
      <th> Depart Date</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(flight => { %>
      <tr>
        <td> <%=flight.airline%> </td>
        <td> <%=flight.airport%> </td>
        <td> <%=flight.flightNo%> </td>
        <td> <%=flight.departs%> </td>
      </tr>
      <% }) %>
  </tbody>
  </table>
<%- include('../partials/footer.ejs') %>